             filter="select Name, PercentFreeSpace, AvgDisksecPerRead, AvgDisksecPerWrite, AvgDisksecPerTransfer, DiskBytesPersec, DiskTransfersPersec from Win32_PerfFormattedData_PerfDisk_LogicalDisk where Name='C:'"
