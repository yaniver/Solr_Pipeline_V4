		<add name="db_dmv_top_queries_sqlextractor_n5" type="sql" filter="SELECT prc.spid,ses.program_name, ses.status, LEFT(t.[text], 50) AS [ShortQueryText], qs.execution_count AS [ExecutionCount],LEFT(SUBSTRING( t.[text], qs.statement_start_offset, qs.statement_end_offset-qs.statement_start_offset),50) AS [statement],qs.total_logical_reads/qs.execution_count AS [AvgLogicalReads],format(qs.total_worker_time/1000/1000/60.0, 'N2') AS [TotalWorkerTimeMin],format(qs.total_worker_time/qs.execution_count/1000/1000.0, 'N2') AS [AvgWorkerTimeSec], format(qs.total_elapsed_time/1000/1000/60.0, 'N2') AS [TotalElapsedTimeMin],qs.total_elapsed_time/qs.execution_count AS [AvgElapsedTime],qs.statement_start_offset,qs.statement_end_offset,qs.plan_generation_num FROM sys.dm_exec_query_stats AS qs WITH (NOLOCK)JOIN sys.sysprocesses prc WITH (NOLOCK)ON prc.sql_handle = qs.sql_handle JOIN sys.dm_exec_sessions ses WITH (NOLOCK) ON prc.spid = ses.session_id CROSS APPLY sys.dm_exec_sql_text(plan_handle) AS t WHERE  prc.program_name ='Sql Extractor' AND qs.total_elapsed_time/qs.execution_count/1000>500 ORDER BY prc.spid DESC OPTION (RECOMPILE);">
		<Tags>
			<add name="spid" value="spid" />
			<add name="status" value="status" />
			<add name="ShortQueryText" value="ShortQueryText" />
			<add name="ExecutionCount" value="ExecutionCount" />
			<add name="statement" value="statement" />
