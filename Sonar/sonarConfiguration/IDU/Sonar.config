			<add name="statement_start_offset" value="statement_start_offset" />
			<add name="statement_end_offset" value="statement_end_offset" />
			<add name="plan_generation_num" value="plan_generation_num" />
			<add name="AvgWorkerTimeSec" value="AvgWorkerTimeSec" />
			<add name="TotalWorkerTimeMin" value="TotalWorkerTimeMin" />
			<add name="TotalElapsedTimeMin" value="TotalElapsedTimeMin" />
		</Tags>
		<Values>
			<add name="AvgElapsedTime" value="auto" />
		</Values>
        </add>
		
		<add name="db_dmv_top_queries_enricher_n6" type="sql" filter="SELECT prc.spid,ses.program_name, ses.status, LEFT(t.[text], 50) AS [ShortQueryText], qs.execution_count AS [ExecutionCount],LEFT(SUBSTRING( t.[text], qs.statement_start_offset, qs.statement_end_offset-qs.statement_start_offset),50) AS [statement],qs.total_logical_reads/qs.execution_count AS [AvgLogicalReads],format(qs.total_worker_time/1000/1000/60.0, 'N2') AS [TotalWorkerTimeMin],format(qs.total_worker_time/qs.execution_count/1000/1000.0, 'N2') AS [AvgWorkerTimeSec], format(qs.total_elapsed_time/1000/1000/60.0, 'N2') AS [TotalElapsedTimeMin],qs.total_elapsed_time/qs.execution_count AS [AvgElapsedTime],qs.statement_start_offset,qs.statement_end_offset,qs.plan_generation_num FROM sys.dm_exec_query_stats AS qs WITH (NOLOCK)JOIN sys.sysprocesses prc WITH (NOLOCK)ON prc.sql_handle = qs.sql_handle JOIN sys.dm_exec_sessions ses WITH (NOLOCK) ON prc.spid = ses.session_id CROSS APPLY sys.dm_exec_sql_text(plan_handle) AS t WHERE  prc.program_name like '%Enricher%' AND qs.total_elapsed_time/qs.execution_count/1000>500 ORDER BY prc.spid DESC OPTION (RECOMPILE);">
