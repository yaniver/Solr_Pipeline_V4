		<add name="wmi_memory_sonar" 
            filter="SELECT Name, AvailableMBytes FROM Win32_PerfFormattedData_PerfOS_Memory" 
            resource="http://schemas.microsoft.com/wbem/wsman/1/wmi/root/cimv2/*" namespace="root\cimv2">
