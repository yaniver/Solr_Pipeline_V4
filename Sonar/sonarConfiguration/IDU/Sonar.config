			<add name="query_id" value="query_id" />
			<add name="plan_id" value="plan_id" />
			<add name="ObjectName" value="ObjectName" />
			<add name="TotalExecutions" value="TotalExecutions" />
			<add name="Avg_CPU_ms" value="Avg_CPU_ms" />
			<add name="Avg_LogicalReads" value="Avg_LogicalReads" />
			<add name="ShortQueryText" value="ShortQueryText" />
